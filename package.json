{
	"name": "typelense",
	"version": "0.1.2",
	"description": "TypeScript error collector for TypeScript projects",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": {
		"typelense": "./dist/cli.js"
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"start": "bun run src/cli.ts",
		"dev": "bun --hot src/cli.ts",
		"build": "bun run build.ts",
		"prepublishOnly": "bun run build",
		"release": "release-it"
	},
	"keywords": [
		"typescript",
		"monorepo",
		"errors",
		"linter",
		"type-checking"
	],
	"author": {
		"name": "Monawwar Abdullah",
		"email": "hello@monawwar.io",
		"url": "https://monawwar.io"
	},
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@types/bun": "latest",
		"release-it": "^19.2.3"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"chalk": "^5.6.2",
		"commander": "^14.0.2",
		"fast-glob": "^3.3.3",
		"ora": "^9.0.0",
		"yaml": "^2.8.2"
	}
}
